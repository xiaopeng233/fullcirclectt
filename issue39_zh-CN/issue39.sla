   <ITEXT CAB="16" CCOLOR="article inner header" CBASE="0" CSTW="-0.1" CSIZE="17" CULP="-0.1" CH="Horses For Courses&#x5;" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Garuda Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
