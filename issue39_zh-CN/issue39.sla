   <ITEXT CAB="0" CCOLOR="Code" CBASE="0" CSTW="-0.1" CSIZE="10" CULP="-0.1" CH="def SearchForARecipeMenu():&#x5;    myscreen.addstr(4,1, &quot;-------------------------------&quot;)&#x5;    myscreen.addstr(5,1, &quot; Search in&quot;)&#x5;    myscreen.addstr(6,1, &quot;-------------------------------&quot;)&#x5;    myscreen.addstr(7,1, &quot; 1 - Recipe Name&quot;)&#x5;    myscreen.addstr(8,1, &quot; 2 - Recipe Source&quot;)&#x5;    myscreen.addstr(9,1, &quot; 3 - Ingredients&quot;)&#x5;    myscreen.addstr(10,1,&quot; 0 - Exit&quot;)&#x5;    myscreen.addstr(11,1,&quot;Enter Search Type -> &quot;)&#x5;    myscreen.refresh()&#x5;&#x5;def InKey2():&#x5;    key = 'X'&#x5;    doloop = 1&#x5;    while doloop == 1:&#x5;        key = myscreen.getch(11,22)&#x5;        myscreen.addch(11,22,key)&#x5;        tmpstr = &quot;Enter text to search in &quot;&#x5;        if key == ord('1'):&#x5;            sstr = &quot;'Recipe Name' for -> &quot;&#x5;            tmpstr = tmpstr + sstr&#x5;            retstring = GetSearchLine(13,1,tmpstr)&#x5;            break&#x5;        elif key == ord('2'):&#x5;            sstr = &quot;'Recipe Source' for -> &quot;&#x5;            tmpstr = tmpstr + sstr&#x5;            retstring = GetSearchLine(13,1,tmpstr)&#x5;            break&#x5;        elif key == ord('3'):&#x5;            sstr = &quot;'Ingredients' for -> &quot;&#x5;            tmpstr = tmpstr + sstr&#x5;            retstring = GetSearchLine(13,1,tmpstr)&#x5;            break&#x5;        else:&#x5;            retstring = &quot;&quot;&#x5;            break&#x5;    if retstring != &quot;&quot;:&#x5;        myscreen.addstr(15,1,&quot;You entered - &quot; + retstring)&#x5;    else:&#x5;        myscreen.addstr(15,1,&quot;You entered a blank string&quot;)&#x5;    myscreen.refresh()&#x5;    myscreen.addstr(20,1,&quot;Press a key&quot;)&#x5;    myscreen.getch()&#x5;&#x5;def GetSearchLine(row,col,strng):&#x5;    myscreen.addstr(row,col,strng)&#x5;    myscreen.refresh()&#x5;    instring = myscreen.getstr(row,len(strng)+1)&#x5;    myscreen.addstr(row,len(strng)+1,instring)&#x5;    myscreen.refresh()&#x5;    return instring" CSHADE2="100" CKERN="0" CSCALE="100" CSHADE="100" COUT="1" CSCALEV="100" CSTP="-0.1" CULW="-0.1" CSTYLE="0" CFONT="Courier 10 Pitch Bold" CSHX="5" CSHY="-5" CSTROKE="Black" />
